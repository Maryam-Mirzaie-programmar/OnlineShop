@model IEnumerable<Datalayer.Feature>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FeatureTitle)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FeatureTitle)
            </td>
            <td>
                <a class="tbtn btn-sm btn-warning" onclick="Edit(@item.FeatureId)"><i class="fa fa-edit"></i> </a>
                <a class="tbtn btn-sm btn-danger" onclick="Delete(@item.FeatureId)"><i class="fa fa-trash"></i> </a>
            </td>
        </tr>
    }
</table>
